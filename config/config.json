        "reporting_interval": 60
    },
    "rule": {
        "max_rules": 100
    },
    "gpu": {
        "max_memory_usage": 0.8,
        "monitoring_enabled": true
    },
    "ml": {
        "training_iterations": 1000
    },
    "log": {
        "level": "info"
    },
    "db_manager": {
        "connection_timeout": 30,
        "pool_size": 10
    },
    "auth": {
        "two_factor_enabled": true,
        "social_login": true
    },
    "api": {
        "version": "v1",
        "base_url": "https://api.com"
    },
    "social_engineering": {
        "data_sources": ["facebook", "twitter", "linkedin"]
    },
    "machine_learning": {
        "model_path": "models/ml_model.bin"
    },
    "adaptive_attack": {
        "strategy": "dynamic"
    },
    "recovery": {
        "auto_backup": true
    },
    "utils": {
        "cache_size": 1024
    },
