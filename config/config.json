{
    "db_connection_string": "server=localhost;user=root;password=admin;database=mydb",
    "dictionary_path": "data/dictionaries",
    "rules_path": "data/rules",
    "attack": {
        "thread_count": 10
    },
    "monitoring": {
        "frequency": 5
    },
    "scheduler": {
        "max_tasks": 20
    },
    "webapp": {
        "port": 8080
    },
    "cloud": {
        "timeout": 300,
        "api_url": "https://api.cloudservice.com",
        "auth_token": "token"
    },
    "user_management": {
        "max_users": 500
    },
    "notification": {
        "enabled": true,
        "email_server": "smtp.mailserver.com",
        "sms_gateway": "https://sms.gateway.com",
        "push_server": "https://push.server.com",
        "api_keys": "api_keys"
    },
